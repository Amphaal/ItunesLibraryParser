{
    "configurations": [
        {
            "name": "Win32",
            "browse": {
                "path": [
                    "${env:MINGW64_ROOT}/include"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "includePath": [
                "${workspaceFolder}/src",
                "${workspaceFolder}/build/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "CATCH_CONFIG_ENABLE_BENCHMARKING"
            ],
            "windowsSdkVersion": "10.0.18362.0",
            "compilerPath": "${env:MINGW64_ROOT}/bin/gcc.exe",
            "cStandard": "c11",
            "cppStandard": "c++20",
            "intelliSenseMode": "gcc-x64"
        },
        {
            "name": "MacOS",
            "cStandard": "c11",
            "cppStandard": "c++20",
            "intelliSenseMode": "clang-x64",
            "includePath": [
                "${workspaceFolder}/src",
                "${workspaceFolder}/build/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "CATCH_CONFIG_ENABLE_BENCHMARKING"
            ]
        }
    ],
    "version": 4
}